{
  "name": "anosrv-frontend",
  "version": "0.1.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "start:prod": "ng serve --prod=true",
    "start:metin": "ng serve --configuration=metin",
    "start:entw": "ng serve --configuration=entw",
    "start:test": "ng serve --configuration=test",
    "build:prod": "ng build --prod",
    "build:stats": "ng build --stats-json",
    "test": "ng test --code-coverage",
    "test:ci": "ng test -c ci",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "prettier": "prettier --write \"{src,e2e}/**/*.{js,ts,html,css,scss}\"",
    "analyze": "webpack-bundle-analyzer dist/anosrv-frontend/stats-es2015.json",
    "sonar:ci": "sonar-scanner",
    "---------------------------------------------------------------": "",
    "maven.build": "npm run test:ci && npm run lint && npm run build:prod --deleteOutputPath=true"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~10.0.3",
    "@angular/common": "~10.0.3",
    "@angular/compiler": "~10.0.3",
    "@angular/core": "~10.0.3",
    "@angular/forms": "~10.0.3",
    "@angular/platform-browser": "~10.0.3",
    "@angular/platform-browser-dynamic": "~10.0.3",
    "@angular/router": "~10.0.3",
    "@ng-select/ng-select": "5.0.3",
    "@ngx-translate/core": "13.0.0",
    "@ngx-translate/http-loader": "6.0.0",
    "hammerjs": "2.0.8",
    "keen-slider": "5.4.0",
    "ngx-translate-multi-http-loader": "3.0.0",
    "rxjs": "~6.6.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1000.1",
    "@angular/cli": "^10.0.1",
    "@angular/compiler-cli": "~10.0.3",
    "@types/jasmine": "~3.5.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^5.1.2",
    "dependency-cruiser": "9.9.1",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.0",
    "karma": "~5.0.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-firefox-launcher": "1.3.0",
    "karma-jasmine": "~3.3.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "karma-junit-reporter": "2.0.1",
    "miragejs": "0.1.41",
    "prettier": "2.0.5",
    "protractor": "~7.0.0",
    "sonar-scanner": "3.1.0",
    "ts-node": "~8.10.2",
    "tslint": "~6.1.0",
    "typescript": "~3.9.6",
    "webpack-bundle-analyzer": "3.8.0"
  },
